<h1>Listing fnss_simple_full_meshes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Status</th>
      <th>Full mesh node</th>
      <th>Generator</th>
      <th>Type</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fnss_simple_full_meshes.each do |fnss_simple_full_mesh| %>
      <tr>
        <td><%= fnss_simple_full_mesh.name %></td>
        <td><%= fnss_simple_full_mesh.description %></td>
        <td><%= fnss_simple_full_mesh.status %></td>
        <td><%= fnss_simple_full_mesh.full_mesh_node %></td>
        <td><%= fnss_simple_full_mesh.generator %></td>
        <td><%= fnss_simple_full_mesh.type %></td>
        <td><%= fnss_simple_full_mesh.user %></td>
        <td><%= link_to 'Show', fnss_simple_full_mesh %></td>
        <td><%= link_to 'Edit', edit_fnss_simple_full_mesh_path(fnss_simple_full_mesh) %></td>
        <td><%= link_to 'Destroy', fnss_simple_full_mesh, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fnss simple full mesh', new_fnss_simple_full_mesh_path %>
