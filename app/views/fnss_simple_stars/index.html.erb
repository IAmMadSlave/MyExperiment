<h1>Listing fnss_simple_stars</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Status</th>
      <th>Star node</th>
      <th>Generator</th>
      <th>Type</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fnss_simple_stars.each do |fnss_simple_star| %>
      <tr>
        <td><%= fnss_simple_star.name %></td>
        <td><%= fnss_simple_star.description %></td>
        <td><%= fnss_simple_star.status %></td>
        <td><%= fnss_simple_star.star_node %></td>
        <td><%= fnss_simple_star.generator %></td>
        <td><%= fnss_simple_star.type %></td>
        <td><%= fnss_simple_star.user %></td>
        <td><%= link_to 'Show', fnss_simple_star %></td>
        <td><%= link_to 'Edit', edit_fnss_simple_star_path(fnss_simple_star) %></td>
        <td><%= link_to 'Destroy', fnss_simple_star, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fnss simple star', new_fnss_simple_star_path %>
